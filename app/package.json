{
  "name": "cumulo-workflow",
  "version": "0.1.0",
  "description": "Stack Workflow",
  "main": "index.js",
  "scripts": {
    "cdn": "ls -1 dist | xargs -I % upx put dist/% cumulo-editor/%",
    "tiye": "rsync dist/index.html tiye.me:repo/Cirru/cumulo-editor",
    "upload": "yarn cdn && yarn tiye",
    "release": "shadow-cljs release browser",
    "watch": "shadow-cljs watch browser",
    "compile-ssr": "shadow-cljs compile ssr",
    "html-dev": "env=dev node target/ssr.js",
    "html": "node target/ssr.js",
    "hmr": "webpack-dev-server --config webpack.dev.coffee --hot-only",
    "webpack": "webpack --config webpack.release.coffee --hide-modules --progress",
    "dev": "yarn compile-ssr && yarn html-dev && yarn hmr",
    "del": "rm -rf dist/*",
    "serve": "http-server dist",
    "build": "yarn del && yarn release && yarn webpack && yarn compile-ssr && yarn html"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mvc-works/stack-workflow.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "devDependencies": {
    "coffeescript": "^2.0.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.3",
    "http-server": "^0.10.0",
    "shadow-cljs": "^2.0.7",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1",
    "webpack-manifest-plugin": "^1.3.1"
  },
  "dependencies": {}
}
