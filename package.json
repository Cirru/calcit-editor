  "devDependencies": {
    "feather-icons": "^4.28.0",
    "shadow-cljs": "^2.10.19",
    "source-map-support": "^0.5.19",
    "url-parse": "^1.4.7"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "dayjs": "^1.8.31",
    "express": "^4.17.1",
    "gaze": "^1.1.3",
    "latest-version": "^5.1.0",
    "md5": "^2.2.1",
    "node-notifier": "7.0.2",
    "nrepl-client": "^0.3.0",
    "serve-index": "^1.9.1",
    "shortid": "^2.2.15",
    "ws": "^7.3.1"
