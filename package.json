{
  "name": "calcit-editor",
  "version": "0.4.3",
  "description": "Cirru Calcit Editor",
  "bin": {
    "calcit-editor": "dist/server.js",
    "ce": "dist/server.js"
  },
  "scripts": {
    "watch": "shadow-cljs watch client server",
    "serve": "http-server dist -s",
    "repl": "rlwrap shadow-cljs clj-repl",
    "build-cdn": "shadow-cljs clj-run build.main/build-cdn",
    "page": "shadow-cljs clj-run build.main/page",
    "upload": "node target/upload.js",
    "up": "yarn build-cdn && yarn upload",
    "build": "shadow-cljs clj-run build.main/build",
    "ln": "cd target && rm -f entry && ln -s ../entry"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Cirru/calcit-editor.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "devDependencies": {
    "feather-icons": "^4.17.0",
    "http-server": "^0.11.1",
    "shadow-cljs": "^2.8.10",
    "source-map-support": "^0.5.10",
    "url-parse": "^1.4.4"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "dayjs": "^1.8.7",
    "express": "^4.16.4",
    "gaze": "^1.1.3",
    "latest-version": "^4.0.0",
    "md5": "^2.2.1",
    "serve-index": "^1.9.1",
    "shortid": "^2.2.14",
    "ws": "^6.1.4"
  }
}
